{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prepare": "node -e \"const fs=require('fs');const cp=require('child_process');if(!fs.existsSync('../.git'))process.exit(0);try{cp.execSync('git -C .. config --local core.hooksPath .githooks',{stdio:'inherit'})}catch(_){process.exit(0)}\"",
    "build": "vite build",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:coverage": "npm run test:e2e:coverage:console",
    "test:e2e:coverage:console": "playwright test --reporter=line"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.13",
    "jspdf": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.6.0",
    "svg2pdf.js": "^2.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.7",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "istanbul-lib-coverage": "^3.2.2",
    "jsdom": "^27.0.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.8.3",
    "v8-to-istanbul": "^9.3.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.7"
  }
}
